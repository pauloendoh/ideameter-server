  "name": "ideameter-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "tsnd --respawn src/server.ts",
    "build": "tsc && prisma generate",
    "postinstall": "tsc && prisma generate",
    "start": "node build/src/server.js",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "npm run docker:up && dotenv -e .env.test -- npx prisma migrate deploy && npx vitest run",
    "test:coverage": "npm run test --coverage",
    "test:watch": "npm run docker:up && dotenv -e .env.test -- npx prisma migrate deploy && npx vitest",
    "prepare": "husky install"
  },
